{
  "groups": {
    "CONTAINERS": [
      "^/containers/json$",
      "^/containers/[a-f0-9]{12,64}/json$"
    ],
    "SERVICES": [
      "^/services$",
      "^/services/[a-zA-Z0-9]{25}$",
      "^/services/[a-zA-Z0-9]{25}/json$"
    ],
    "TASKS": [
      "^/tasks$",
      "^/tasks/[a-zA-Z0-9]{25}$"
    ],
    "NETWORKS": [
      "^/networks$",
      "^/networks/[a-zA-Z0-9]{25}$"
    ],
    "INFO": [
      "^/info$",
      "^/version$"
    ],
    "EVENTS": [
      "^/events$"
    ]
  },
  "global_deny": [
    "^/containers/[a-f0-9]{12,64}/attach",
    "^/containers/[a-f0-9]{12,64}/logs",
    "^/containers/[a-f0-9]{12,64}/exec",
    "^/exec/[a-zA-Z0-9]{25}/start",
    "^/exec/[a-zA-Z0-9]{25}/resize"
  ]
}
